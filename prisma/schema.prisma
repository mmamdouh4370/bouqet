generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String         @id
  bouquets SavedBouquet[]
}

model SavedBouquet {
  id      Int    @id @default(autoincrement())
  bouquet String
  userId  String
  user    User   @relation(fields: [userId], references: [id])
}
