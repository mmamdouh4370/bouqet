generator client {
  provider             = "python3 -m prisma"
  recursive_type_depth = "5"
  interface            = "sync"
  output               = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String         @id
  bouquets SavedBouquet[]
}

model SavedBouquet {
  id      Int    @id @default(autoincrement())
  bouquet String
  userId  String
  user    User   @relation(fields: [userId], references: [id])
}
